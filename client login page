<script>
	  function check()
				  {
						  if(f.eno.value=="")
							 {
								  alert("enter your id");
								  f.eno.focus();
								  return false;
							 }
							 else if(isNaN(f.eno.value))
							{
								  alert("enter digits only");
								  f.eno.focus();
								  return false;
							 }
						  if(f.pwd.value=="")
							 {
								  alert("enter pwd");
								  f.pwd.focus();
								  return false;
							  }
						 return true;
				  }
  </script>
